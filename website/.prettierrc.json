{
    "$schema": "http://json.schemastore.org/prettierrc",
    "useTabs": false,
    "tabWidth": 4,
    "singleQuote": false,
    "trailingComma": "es5",
    "semi": true,
    "printWidth": 100,
    "overrides": [
        {
            "files": "*.svg",
            "options": {
                "parser": "html"
            }
        }
    ]
}
